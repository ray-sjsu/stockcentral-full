generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model user {
  userId        Int     @id @unique(map: "sqlite_autoindex_user_1") @default(autoincrement())
  username      Int     @unique(map: "sqlite_autoindex_user_2")
  password      Int
  currentStock  String?
  searchHistory String?
  favoriteList  String?
  isLoggedIn    String
}
