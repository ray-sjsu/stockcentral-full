{
  "name": "stockcentral",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "scripts": {
    "flask-dev": "cd flask-api && python -m venv venv && pip install -r requirements.txt && set FLASK_APP=app.py && set FLASK_ENV=development && set FLASK_DEBUG=1 && python -m flask run",
    "next-dev": "npm install && next dev",
    "db": "npx prisma studio",
    "db-reset": "npx prisma migrate reset",
    "build": "npm install && npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.12.1",
    "bcrypt": "^5.1.1",
    "chart.js": "^4.4.2",
    "chartjs-plugin-zoom": "^2.0.1",
    "iron-session": "^8.0.1",
    "jose": "^5.2.4",
    "next": "^14.2.3",
    "next-auth": "^4.24.7",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18",
    "react-hook-form": "^7.51.2",
    "react-icons": "^5.0.1",
    "react-toastify": "^10.0.5",
    "sharp": "^0.33.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "postcss": "^8",
    "prisma": "^5.12.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
